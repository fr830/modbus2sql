    public void SetupForm(SQLCommand SqlConnect) {
        LocalSQLConnection = SqlConnect;

        //Build the spinner model for 1-16 bits
        SpinnerModel Spinnermodel1 = new SpinnerNumberModel(1,
                1, //min
                16, //max
                1);                //step
        spin_warning_bit.setModel(Spinnermodel1);

        SpinnerModel Spinnermodel2 = new SpinnerNumberModel(1,
                1, //min
                16, //max
                1);                //step
        spin_shutdown_bit.setModel(Spinnermodel2);

        SpinnerModel Spinnermodel3 = new SpinnerNumberModel(1,
                1, //min
                16, //max
                1);                //step
        spin_tripped_bit.setModel(Spinnermodel3);


        SpinnerModel Spinnermodel4 = new SpinnerNumberModel(1,
                1, //min
                16, //max
                1);                //step
        spin_sendreset_bit.setModel(Spinnermodel4);


        SpinnerModel Spinnermodel5 = new SpinnerNumberModel(1,
                1, //min
                16, //max
                1);                //step
        spin_recvreset_bit.setModel(Spinnermodel5);

        SpinnerModel Spinnermodel6 = new SpinnerNumberModel(2,
                2, //min
                16, //max
                1);                //step
        spin_slave_id.setModel(Spinnermodel6);
    }

    @Action
    public void addNewAlarmsAnnunciator() {
        Integer warningregister = (Integer) spin_warning_reg.getValue();
        Integer warningbit = (Integer) spin_warning_bit.getValue();
        Integer shutdownregister = (Integer) spin_shutdown_reg.getValue();
        Integer shutdownbit = (Integer) spin_shutdown_bit.getValue();
        Integer tripregister = (Integer) spin_tripped_reg.getValue();
        Integer tripbit = (Integer) spin_tripped_bit.getValue();
        Integer slaveid = (Integer) spin_slave_id.getValue();
        Integer sendresetreg = (Integer) spin_sendreset_reg.getValue();
        Integer sendresetbit = (Integer) spin_sendreset_bit.getValue();
        Integer recvresetreg = (Integer) spin_recvreset_reg.getValue();
        Integer recvresetbit = (Integer) spin_recvreset_bit.getValue();

        String sqlcmd = "INSERT INTO alarmannunciator SET warnregister=" + warningregister + ", "
                + "warnregisterbit=" + warningbit + ", "
                + "shutdownregister=" + shutdownregister + ", "
                + "shutdownregisterbit=" + shutdownbit + ", "
                + "tripregister=" + tripregister + ", "
                + "tripregisterbit=" + tripbit + ", "
                + "slaveID=" + slaveid + ", "
                + "sendresetflag=0, "
                + "sendresetreg=" + sendresetreg + ", "
                + "sendresetbit=" + sendresetbit + ", "
                + "recvresetflag=0, "
                + "recvresetreg=" + recvresetreg + ", "
                + "recvresetbit=" + recvresetbit + ";";

        LocalSQLConnection.SQLUpdateCommand(sqlcmd);
        this.dispose();
    }

    @Action
    public void btn_close_action() {
        this.dispose();
    }
